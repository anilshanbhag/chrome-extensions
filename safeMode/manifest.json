{
  "name": "Safe Mode",
  "version": "0.1",
  "description": "Browse the web without having history of unwanted sites recorded",
  "permissions": ["history", "tabs"],
  "icons": {
      "128": "logo128.png",
      "16": "logo16.png",
      "32": "logo32.png",
      "48": "logo48.png"
   },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "clock.png"
  },
  "background" : {
    "scripts": ["history_safe.js"]
  }
}
